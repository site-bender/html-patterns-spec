{
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"endOfLine": "lf",
	"jsxSingleQuote": false,
	"overrides": [
		{
			"files": "*.html",
			"options": {
				"parser":"html"
			}
		}
	],
	"parser": "typescript",
	"printWidth": 80,
	"proseWrap": "always",
	"quoteProps": "consistent",
	"semi": false,
	"tabWidth": 2,
	"trailingComma": "all",
	"useTabs": true
}
